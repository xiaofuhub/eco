global function boolean f_validate_recagcy (string Lrecagcy);int rowcnt=0
SELECT count(*) into :rowcnt from narrtbl 
where recagcy=:lrecagcy
using sqlservertrans;
IF rowcnt=0 THEN
	RETURN FALSE
ELSE
	RETURN TRUE
END IF
end function

